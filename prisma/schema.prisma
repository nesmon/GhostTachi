generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MaimaiFinale {
  id              Int      @id @default(autoincrement())
  username        String   @unique
  userData        Json?
}

